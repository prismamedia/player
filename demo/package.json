{
	"name": "prisma-player-demo",
	"private": true,
	"scripts": {
		"biome:check": "biome check",
		"biome:fix": "biome check --write",
		"build": "rm -rf ./dist && webpack --mode=production --config config/webpack.config.js",
		"dev": "rm -rf ./dist && webpack serve --mode=development --config config/webpack.config.js",
		"dev:types": "tsc --watch --noEmit",
